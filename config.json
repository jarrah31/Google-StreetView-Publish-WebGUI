{
  "app": {
    "debug": true,
    "host": "0.0.0.0"
  },
  "logging": {
    "level": "INFO",
    "file": "logs/streetview.log",
    "max_bytes": 1048576,
    "backup_count": 10,
    "format": "%(asctime)s %(levelname)s: %(message)s [in %(pathname)s:%(lineno)d]"
  },
  "uploads": {
    "directory": "uploads",
    "allowed_extensions": ["jpg", "jpeg"],
    "max_file_size": 67108864
  },
  "api": {
    "places": {
      "max_results": 60,
      "default_radius": 300
    },
    "photos": {
      "default_page_size": 10,
      "table_page_size": 100,
      "max_nearby_photos": 50
    }
  }
}
